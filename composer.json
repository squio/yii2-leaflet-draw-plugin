{
    "name": "davidjeddy/yii2-leaflet-draw-plugin",
    "description": "Plugin for the 2amigo Leaflet extension that adds the Leaflet.Draw functionality.",
    "keywords": [
        "davidjeddy",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "maps",
        "leaflet",
        "plugin",
        "icon"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "David J Eddy",
            "email": "me@davidjeddy.us",
            "homepage": "http://davidjeddy.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/davidjeddy/yii2-leaflet-draw-plugin/issues",
        "source": "https://github.com/davidjeddy/yii2-leaflet-draw-plugin"
    },
    "require": {
        "2amigos/yii2-leaflet-extension" : "~1.0",
        "bower-asset/leaflet.draw": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "davidjeddy\\leaflet\\plugins\\draw\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
